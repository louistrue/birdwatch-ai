# Bird Watcher Configuration
# Copy this file to .env and fill in your values

# Camera Settings
CAMERA_IP=192.168.1.100
CAMERA_USER=admin
CAMERA_PASSWORD=your_password_here
CAMERA_RTSP_MAIN=rtsp://${CAMERA_USER}:${CAMERA_PASSWORD}@${CAMERA_IP}:554/stream1
CAMERA_RTSP_SUB=rtsp://${CAMERA_USER}:${CAMERA_PASSWORD}@${CAMERA_IP}:554/stream2

# BirdNET Location (for species filtering)
LATITUDE=47.37
LONGITUDE=8.54

# Audio Settings
ENABLE_AUDIO=true
AUDIO_DEVICE=plughw:2,0
